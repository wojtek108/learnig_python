print('X. Jak masz na imie?')
name = input()

while name.isalpha() == False:
  print('Hm... To nie wyglada mi na prawdziwe imie. Podaj swoje prawdziwe imie')
  name = input()
  if name.isalpha() == True:
    break

print('Milo Cie poznac ' + str(name))
print('AAA. Ile masz lat?')
age = int(input())

print('END')
